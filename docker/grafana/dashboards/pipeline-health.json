  "panels": [
    {
      "title": "Kafka Consumer Lag per Topic",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "kafka_consumer_group_lag{topic=~\"sales.*\"}",
          "legendFormat": "{{topic}} - {{partition}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "short"
        },
        "overrides": []
      }
    },
    {
      "title": "Flink Job Throughput (records/sec)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(flink_taskmanager_job_task_numRecordsInPerSecond[1m])",
          "legendFormat": "{{task_name}} in/sec"
        },
        {
          "expr": "rate(flink_taskmanager_job_task_numRecordsOutPerSecond[1m])",
          "legendFormat": "{{task_name}} out/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "ops"
        },
        "overrides": []
      }
    },
    {
      "title": "Flink Checkpoint Duration",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "flink_jobmanager_job_lastCheckpointDuration",
          "legendFormat": "{{job_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50
          },
          "unit": "ms"
        },
        "overrides": []
      }
    },
    {
      "title": "ClickHouse Insert Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(ClickHouseProfileEvents_InsertedRows[1m])",
          "legendFormat": "rows/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20
          },
          "unit": "ops"
        },
        "overrides": []
      }
    },
    {
      "title": "API Request Latency (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket{job=\"results-api\"}[5m]))",
          "legendFormat": "{{uri}} p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "title": "Error Rate Across Components",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count{status=~\"5..\", job=\"results-api\"}[5m])",
          "legendFormat": "API 5xx"
        },
        {
          "expr": "rate(flink_taskmanager_job_task_numRecordsInErrors[1m])",
          "legendFormat": "Flink errors"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 30
          },
          "unit": "ops",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      }
    }
